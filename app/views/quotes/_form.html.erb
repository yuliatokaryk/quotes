<%= form_with(model: @quote, local:true) do |f| %>
  <%= f.label :content %><br>
  <%= f.text_field :content %><br>
        
  <%= f.label :book_id, class: 'form-label' %><br>
  <%= f.select :book_id, Book.all.collect {|b| [ b.title, b.id ] }, { include_blank: true } %><br>
        
  <%= f.submit %>
<% end %>
